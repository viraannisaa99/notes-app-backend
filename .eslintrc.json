{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "max-len": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "padded-blocks": "off",
    "no-undef": "off",
    "object-curly-newline": "off"
  }
}
